{
  "name": "type-bull",
  "version": "1.0.11",
  "description": "Nest - modern, fast, powerful node.js web framework (type-bull)",
  "author": "Denis Vakhtenkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Celbine/type-bull/issues"
  },
  "readmeFilename": "README.md",
  "homepage": "https://github.com/Celbine/type-bull#readme",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint lib/**/*.ts --fix",
    "release": "release-it",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "@nestjs/common": "^8.0.4",
    "@types/bull": "^3.15.2",
    "@nestjs/core": "^8.0.4",
    "@types/node": "^16.4.1",
    "@types/uuid": "^8.3.1",
    "husky": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "lint-staged": "^11.1.0",
    "prettier": "^2.3.2",
    "bull": "^3.26.0",
    "reflect-metadata": "^0.1.13",
    "release-it": "^14.10.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Celbine/type-bull.git"
  },
  "peerDependencies": {
    "bull": "^3.26.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "keywords": [
    "bull",
    "decorators",
    "nestjs",
    "decorators",
    "type-bull",
    "typescript",
    "bulljs"
  ],
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
